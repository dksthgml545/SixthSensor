<?xml version="1.0" encoding="utf-8"?>

<shape
    xmlns:xmls="http://schemas.android.com/apk/res/android"
    xmls:shape="rectangle"
    xmls:visible="true"
    xmls:dither="true"
    xmls:useLevel="false"
    >
    <gradient
        xmls:startColor="@color/colorButton"
        xmls:endColor="@color/colorButton"
        />
    <corners
        xmls:radius="3dp"
        />
    <size
        xmls:width="50dp"
        xmls:height="30dp"
        />
    <stroke
        xmls:color="@color/colorButton"
        xmls:width="5dp"
        />
</shape>